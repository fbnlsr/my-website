    "eleventy-plugin-emojify": "^1.0.1",
    "eleventy-plugin-reading-time": "^0.0.1",
    "esbuild": "^0.24.2",
    "markdown-it": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.93.2"
  },
  "dependencies": {
    "@glidejs/glide": "^3.7.1",
    "highlight.js": "11.10.0",
    "html-minifier-next": "^3.2.1",
    "vanilla-lazyload": "^19.1.3"
